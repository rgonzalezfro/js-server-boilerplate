{
  "name": "js-server-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for webapp with nodejs server",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel run:server lint:watch test:watch",
    "run:server": "babel-node buildScripts/srcServer.js",
    "lint":"esw webpack.config.* src buildScripts --color",
    "lint:watch":"npm run lint -- --watch",
    "test": "mocha --reporter progress buildScripts/testSetup.js 'src/**/*.test.js'",
    "test:watch": "npm test -- --watch",
    "localtunnel": "lt -h 'http://serverless.social' -p 3000",
    "share": "npm-run-all --parallel start localtunnel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rgonzalezfro/js-server-boilerplate.git"
  },
  "keywords": [],
  "author": "Rafael Gonzalez",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "babel-loader": "8.1.0",
    "chai": "4.2.0",
    "chalk": "4.0.0",
    "cheerio": "0.22.0",
    "compression": "1.7.4",
    "cross-env": "7.0.2",
    "css-loader": "3.5.1",
    "eslint": "6.8.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-watch": "6.0.1",
    "express": "4.17.1",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "4.0.4",
    "jsdom": "16.2.2",
    "json-schema-faker": "0.4.7",
    "json-server": "0.16.1",
    "localtunnel": "2.0.0",
    "mocha": "7.1.1",
    "nock": "12.0.3",
    "npm-run-all": "4.1.5",
    "nsp": "3.2.1",
    "numeral": "2.0.6",
    "open": "7.0.3",
    "rimraf": "3.0.2",
    "style-loader": "1.1.3",
    "surge": "0.21.3",
    "webpack": "4.42.1",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "2.25.0",
    "webpack-md5-hash": "0.0.6"
  },
  "bugs": {
    "url": "https://github.com/rgonzalezfro/js-server-boilerplate/issues"
  },
  "homepage": "https://github.com/rgonzalezfro/js-server-boilerplate#readme"
}
